@using Services.utils
<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Email Timeline, Initiative Timeline Generator, NHLBI, NIH</title>
   @*  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ITG_vs.styles.css" asp-append-version="true" /> *@

    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Initiative Timeline Generator, NHLBI, NIH</title>
    <link rel="shortcut icon" href="https://www.nhlbi.nih.gov/favicon.ico" type="image/x-icon" />
    <script src="https://code.jquery.com/jquery-3.6.0.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/js/d3.min.js"></script>
    @RenderSection("css", false)
</head>
<body>
  
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Bootstrap -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Organization header styles -->
    <link rel="stylesheet" href="~/css/header-styles.css" type="text/css" />
    <!-- Font Awesome -->
    <link href="~/css/font-awesome.min.css" rel="stylesheet" rel="stylesheet"/>
    <script src="https://code.jquery.com/jquery-3.6.0.js" type="text/javascript"></script>
    <script src="~/js/bootstrap.bundle.min.js" type="text/javascript"></script>
    <script src="~/js/jquery.trap.min.js" type="text/javascript"></script>
    <script src="~/js/popover-508.js" type="text/javascript"></script>
   
    <script type="text/javascript">

        $(document).ready(function () {

            // keeps focus in modals while open and returns focus to the control that opened the modal
            function bindModalEvents() {
                var lastActiveElement;
                $(".modal").on("show.bs.modal", function () {
                    lastActiveElement = $(document.activeElement);
                });

                $(".modal").on("hidden.bs.modal", function () {
                    lastActiveElement.focus();
                });

                $(".modal").on("shown.bs.modal", function () {
                    $(this).trap();
                });

                $(".modal").on("hide.bs.modal", function () {
                    $(this).untrap();
                });
            }

            bindModalEvents();
        });

        function redirectHome() {
            window.location.href = "index";
        }
    </script>

    <!-- Place Web Analytics tracking code HERE - To collect data, you must copy and paste the Analytics tracking code into the source code on every web page you wish to track. -->
    <script src="@Settings.Get("AnalyticsExternalFile")"></script>
 
    <div class="container-fluid d-none d-md-block">
        <div class="row organization-background">
            <div class="col-sm-7">
                <a title="Home" rel="home" href="#" class="text-nounderline text-uppercase header-secondary logo-header-text"><strong>NHLBI</strong> <span class="header-primary text-uppercase">ITG</span></a>
            </div>
            <div class="col-sm-5 text-right pt-1 d-none d-md-block">
            </div>
        </div>
    </div>
    <nav class="navbar navbar-expand-md navbar-static-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggler pull-left ml-1 mt-1" data-toggle="collapse" data-target="#main-navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="fa fa-bars fa-lg" aria-hidden="true"></span>
            </button>
            <a href="#" class="navbar-brand d-block d-sm-block d-md-none text-nounderline text-uppercase header-secondary" title="Home" rel="home"><strong>NHLBI</strong> <span class="header-primary text-uppercase">ITG</span></a>
            <div class="navbar-header">
            </div>
        </div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="main-navbar-collapse">
            <ul class="navbar-nav">
                <li class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown" id="mainDropdown" role="button" aria-haspopup="true" aria-expanded="false"><strong>ITG </strong><span class="caret"></span></a>
                    <div class="dropdown-menu" aria-labelledby="mainDropdown">
                        <a href="#aboutITGModal" class="dropdown-item" data-toggle="modal" data-target="#aboutITGModal" id="aboutITGPage">About ITG</a>
                        <a href="#itgReleaseNotesModal" class="dropdown-item" data-toggle="modal" data-target="#itgReleaseNotesModal" id="itgReleaseNotesPage">Release Notes</a>
                        <a href="#itgSystemRequirementsModal" class="dropdown-item" data-toggle="modal" data-target="#itgSystemRequirementsModal" id="itgSystemRequirementsPage">System Requirements</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a href="#" onclick="redirectHome()" class="nav-link">Home</a>
                </li>
                <li class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown" id="helpDropdown" role="button" aria-haspopup="true" aria-expanded="false">Help <span class="caret"></span></a>
                    <div class="dropdown-menu" aria-labelledby="helpDropdown">
                        <a target="_blank" class="dropdown-item" href="https://myitsm.nih.gov/navpage.do">Contact IT Service Desk</a>
                    </div>
                </li>
            </ul>
        </div>
        <!-- /.navbar-collapse -->
        <!-- /.container -->
        <!--optional breadcrumb -->
        <!-- begin breadcrumb -->
    </nav>
    <nav id="breadcrumb" aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#" onclick="redirectHome()" title="Home" aria-label="home icon"><i class="fa fa-home fa-lg" aria-hidden="true"></i></a></li>
            <li class="breadcrumb-item"><a href="https://intranet.nhlbi.nih.gov/">NHLBI Intranet</a></li>
            <li class="breadcrumb-item"><a href="https://intranet.nhlbi.nih.gov/general/dera-od-staff-and-office-directors">DERA</a></li>
            <li class="breadcrumb-item active"><a href="#" onclick="redirectHome()">Initative Timeline Generator</a></li>
        </ol>
    </nav>
  
    @RenderBody()  

    
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script type="text/javascript">@Settings.Get("AnalyticsFooterScript")</script>
    @await RenderSectionAsync("Scripts", required: false)


    <div id="itgSystemRequirementsModal" class="modal fade" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="itgSystemRequirementsModalLabel" aria-describedby="itgSystemRequirementsModalDescription">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 id="itgSystemRequirementsModalLabel" class="modal-title">System Requirements</h5>
                </div>
                <div id="itgSystemRequirementsModalDescription">
                    <div class="modal-body">
                        <div class="justify-content-center align-items-center">
                            <h2>ITG System Requirements</h2>
                            <h4>ITG Supported Web Browsers</h4>
                            <br />
                            Click to view the list of <a href="https://www.nhlbi.nih.gov/digital/browsers.html" target="_blank">supported browsers.</a>
                            <hr>
                            <h4>Internet Explorer Compatibility Mode Settings</h4>
                            <p>
                                To ensure the full functionality of NHLBI applications using Internet Explorer, please use the following guidance
                                to turn off compatibility mode.
                            </p>
                            <br />
                            1. In the browser, select Tools > Compatibility View Settings to open the settings window.<br />
                            <br />
                            <p>
                                <img src="~/img/compatibility_1.png" alt="Compatibility Mode On">
                            </p>
                            <br />
                            2. Uncheck the boxes "Display all websites in Compatibility View" and "Display intranet sites in Compatibility View" if they are not
                            already unchecked.<br />
                            <br />
                            <p>
                                <img src="~/img/compatibility_2_highlight.png" alt="Compatibility Mode Off">
                            </p>
                            <br />
                            3. Close the settings window and restart your browser before returning to the web application.
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" data-dismiss="modal" aria-label="OK">OK</button>
                        <span class="sr-only">button</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="itgReleaseNotesModal" class="modal fade" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="itgReleaseNotesModalLabel" aria-describedby="itgReleaseNotesModalDescription">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 id="itgReleaseNotesModalLabel" class="modal-title">Release Notes</h5>
                </div>
                <div id="itgReleaseNotesModalDescription">
                    <div class="modal-body">
                        <div class="justify-content-center align-items-center">
                            <h2 class="releaseHeader">ITG Release Notes</h2>
                            <h3 class="releaseHeader">Release 3.3.0</h3>
                            <br>
                            <h4>Welcome</h4>
                            <ul>
                                <li>Revised and shortened text using a clickable "i" to toggle additional details.</li>
                            </ul>
                            <br>
                            <h4>Initiative Types: Grants and Contract</h4>
                            <ul>
                                <li>Removed slider functionality.</li>
                                <li>Print icon activated after selection of Anticipated Council date.</li>
                                <li>Added PDF preview pop-up of summary table and graphical timeline.</li>
                                <li>All past milestone deadlines appear red on PDF preview pop-up.</li>
                                <li>Removed email icons that enabled the timeline to be sent by email.</li>
                                <li>Added new contact details for the Office of FOA Development and Referral with a link to an Outlook email template on the PDF preview pop-up.</li>
                                <li>Removed zoom functionality on graphical timeline.</li>
                            </ul>
                            <br>
                            <h4>Initiative Type: Grants</h4>
                            <ul>
                                <li>Added two new initiative types (PAR, PAS).</li>
                                <li>Adjusted Anticipated Council dates for selection to reflect actual decision-making session dates. </li>
                                <li>Added new pre-concept clearance milestones: Idea Forum, Round 1 and Round 2, and BEE Meeting.</li>
                                <li>Adjusted existing milestone labels for better clarity and precision.</li>
                                <li>New milestone descriptions displayed using clickable "i".</li>
                                <li>New dwell time default of 60 days in manual entry box, with possible range of 60-120 days.</li>
                                <li>New milestone labels and date format on graphical timeline.</li>
                            </ul>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" data-dismiss="modal" aria-label="OK">OK</button>
                        <span class="sr-only">button</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="aboutITGModal" class="modal fade" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="aboutITGModalLabel" aria-describedby="aboutITGModalDescription">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 id="aboutITGModalLabel" class="modal-title">About ITG</h5>
                </div>
                <div id="aboutITGModalDescription">
                    <div class="modal-body">
                        <div class="justify-content-center align-items-center">
                            <h2>ITG</h2>
                            <h3>Initiative Timeline Generator</h3>
                            <br>
                            <h3>Version 3.3.1</h3>
                            <br>
                            <div>Initiative Timeline Generator is intended to aid in drafting timelines for RFA/RFPs based on specific fiscal year targets and criteria.</div>
                            <br>
                            <img src="images/NHLBI_Standard_Logo_Black.png" alt="NHLBI logo and HHS logo" style="width:400px;height:100px">
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" data-dismiss="modal" aria-label="OK">OK</button>
                        <span class="sr-only">button</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
</html>